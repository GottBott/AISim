;; Scenario where there are 4 tasks. One task_4 can be accomplished by any agent.
;; The remaining 3 tasks can be accomplished by any of Agent_2, Agent_3, and Agent_4.
;; This tests whether Agent 1 is the agent who accomplishes task 4 which is most efficient. 
;;;;

(spec_version "V2_0")

(spec_agent
  (label Agent_1)
)

(spec_agent
  (label Agent_2)
)

(spec_agent
  (label Agent_3)
)

(spec_agent
  (label Agent_4)
)

(spec_task_group
   (label Problem1)
   (subtasks t1 t2 t3 t4)
   (qaf q_sum)
)

(spec_task
   (label t1)
   (earliest_start_time 0)
   (deadline 50000)
   (subtasks t1-m2 t1-m3 t1-m4)
   (qaf q_or)
)

